    "@babel/preset-env": "^7.5.0",
    "concurrently": "^4.1.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "engines": {
    "node": ">=8.9.4"
  }
